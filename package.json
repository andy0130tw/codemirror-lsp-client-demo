{
  "name": "codemirror-lsp-client-demo",
  "version": "0.0.0",
  "description": "My opinionated SvelteKit template in pure JavaScript.",
  "author": "qbane",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || true",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.app.json",
    "tsc": "tsc -b tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^5.7.2",
    "@sveltejs/kit": "^2.22.2",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@types/node": "^22.15.34",
    "svelte": "^5.34.9",
    "svelte-check": "^4.2.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0 <25.0.0"
  },
  "dependencies": {
    "@andy0130tw/vtsls-language-service": "^0.2.9-dev-browser.6",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lint": "^6.8.5",
    "@codemirror/lsp-client": "^6.0.0",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.0",
    "codemirror": "^6.0.2",
    "patch-package": "^8.0.0",
    "uuid": "^11.1.0",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-protocol": "^3.17.5",
    "vscode-uri": "^3.1.0"
  }
}
